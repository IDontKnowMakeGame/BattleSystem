                            <ui:Label display-tooltip-when-elided="true" text="쌍검은 이동하며 공격하는 무기입니다.&#10;이동하는 방향에 좌 우 에 적이 있다면 데미지가 줍니다." class="text" style="font-size: 40px; -unity-text-align: middle-center; height: 21%; background-color: rgba(0, 0, 0, 0.78);" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="Spear_6" style="justify-content: center; align-items: center; width: 100%; height: 100%; position: absolute; translate: 600% 0;">
                        <ui:VisualElement name="title" style="flex-basis: 20%; flex-direction: row; border-bottom-color: rgb(255, 255, 255); border-bottom-width: 4px; width: 100%;">
                            <ui:VisualElement name="icon" style="background-image: url(&apos;project://database/Assets/02.Sprites/UI/UI_Icon.png?fileID=-875096578&amp;guid=a1cbd5f1f818af548831518a40057cbf&amp;type=3#UI_Icon_4&apos;); height: 100%; -unity-background-scale-mode: scale-to-fit; flex-basis: 10%;" />
