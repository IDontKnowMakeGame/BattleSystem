.image-simple {
    width: 100%;
    height: 77px;
    margin-top: 1px;
    margin-bottom: 1px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: rgba(0, 0, 0, 0.25);
}

.OpenQuest {
    translate: 1px 0;
    transition-duration: 0.4s;
}

.CloseQuest {
    translate: -100% 0;
    transition-duration: 0.4s;
}
