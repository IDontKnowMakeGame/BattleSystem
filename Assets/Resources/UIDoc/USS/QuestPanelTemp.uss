.image-simple {
    width: 100%;
    height: 100px;
    margin-top: 1px;
    margin-bottom: 1px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: rgba(0, 0, 0, 0.25);
}

.OpenQuest {
    transition-property: translate;
    transition-duration: 0.4s;
    transition-timing-function: ease-in;
    transition-delay: 0.1s;
}

.CloseQuest {
    translate: -362px 0;
}
